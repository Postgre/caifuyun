<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/head"
    android:orientation="vertical"
    tools:context="com.zepan.caifuyun.adapter.HomeFragment" >

    <include
        android:id="@+id/include_title"
        layout="@layout/include_title" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/head" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/head"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/fcfcfc"
                android:orientation="horizontal" >

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" >

                    <TextView
                        android:id="@+id/tv_booking_form"
                        style="@style/style757575_13"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:drawablePadding="6dp"
                        android:drawableTop="@drawable/selector_book_from"
                        android:gravity="center"
                        android:paddingBottom="16dp"
                        android:paddingTop="16dp"
                        android:text="预约待报单" />

                    <TextView
                        android:id="@+id/booking_form_num"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="right"
                        android:layout_marginRight="30dp"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/cicle_red"
                        android:padding="3dp"
                        android:text=""
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="10sp" 
                        android:visibility="gone"
                        />
                </FrameLayout>

                <TextView
                    android:id="@+id/tv_form_verifying"
                    style="@style/style757575_13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:drawablePadding="6dp"
                    android:drawableTop="@drawable/selector_examine_from"
                    android:gravity="center"
                    android:paddingBottom="16dp"
                    android:paddingTop="16dp"
                    android:text="报单审核中" />

                <TextView
                    android:id="@+id/tv_form_finish"
                    style="@style/style757575_13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:drawablePadding="6dp"
                    android:drawableTop="@drawable/selector_succeed"
                    android:gravity="center"
                    android:paddingBottom="16dp"
                    android:paddingTop="16dp"
                    android:text="已成功" />
            </LinearLayout>

            <FrameLayout style="@style/lines_d2d2d2" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:background="@color/fcfcfc"
                android:orientation="vertical" >

                <FrameLayout style="@style/lines_d2d2d2" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tv_clues"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:background="@drawable/selectable_background"
                        android:drawableRight="@drawable/ic_arrow_right"
                        android:gravity="center_vertical"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:text="线索" />

                    <TextView
                        android:id="@+id/clues_num"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="100dp"
                        android:layout_marginRight="45dp"
                        android:ellipsize="end"
                        android:gravity="center_vertical|right"
                        android:minHeight="52dp"
                        android:singleLine="true"
                        android:text=""
                        
                        android:textColor="#8d8d8d" />
                </FrameLayout>

                <FrameLayout
                    style="@style/lines_d2d2d2"
                    android:layout_marginLeft="16dp" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tv_customer"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/selectable_background"
                        android:drawableRight="@drawable/ic_arrow_right"
                        android:gravity="center_vertical"
                        android:minHeight="52dp"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:text="客户" />

                    <TextView
                        android:id="@+id/customer_num"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="100dp"
                        android:layout_marginRight="45dp"
                        android:ellipsize="end"
                        android:gravity="center_vertical|right"
                        android:minHeight="52dp"
                        android:singleLine="true"
                        android:text=""
                        
                        android:textColor="#8d8d8d" />
                </FrameLayout>

                <FrameLayout
                    style="@style/lines_d2d2d2"
                    android:layout_marginLeft="16dp" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tv_channels_manager"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:background="@drawable/selectable_background"
                        android:drawableRight="@drawable/ic_arrow_right"
                        android:gravity="center_vertical"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:text="渠道管理" />

                    <TextView
                        android:id="@+id/channels_manager_num"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="100dp"
                        android:layout_marginRight="45dp"
                        android:ellipsize="end"
                        android:gravity="center_vertical|right"
                        android:minHeight="52dp"
                        android:singleLine="true"
                        android:text=""
                       
                        android:textColor="#8d8d8d" />
                </FrameLayout>

                <FrameLayout
                    style="@style/lines_d2d2d2"
                    android:layout_marginLeft="16dp" 
                    android:visibility="gone"/>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" 
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/tv_vip_manager"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/selectable_background"
                        android:drawableRight="@drawable/ic_arrow_right"
                        android:gravity="center_vertical"
                        android:minHeight="52dp"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:text="会员管理" />

                    <TextView
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="100dp"
                        android:layout_marginRight="45dp"
                        android:ellipsize="end"
                        android:gravity="center_vertical|right"
                        android:minHeight="52dp"
                        android:singleLine="true"
                        android:text="6"
                        android:visibility="gone"
                        android:textColor="#8d8d8d" />
                </FrameLayout>

                <FrameLayout style="@style/lines_d2d2d2" 
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="40dp"
                android:layout_marginTop="24dp"
                android:background="@color/fcfcfc"
                android:orientation="vertical" >

                <FrameLayout style="@style/lines_d2d2d2" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tv_customer_remind"
                        style="@style/style393c3d_18"
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:background="@drawable/selectable_background"
                        android:drawableRight="@drawable/ic_arrow_right"
                        android:gravity="center_vertical"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:text="客服助手" />

                    <TextView
                        android:id="@+id/customer_remind_num"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_gravity="center_vertical|right"
                        android:layout_marginRight="45dp"
                        android:visibility="gone"
                        android:background="@drawable/cicle_red" />
                </FrameLayout>

                <FrameLayout
                    style="@style/lines_d2d2d2"
                    android:layout_marginLeft="16dp" />

                <TextView
                    android:id="@+id/tv_sales_journal"
                    style="@style/style393c3d_18"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:background="@drawable/selectable_background"
                    android:drawableRight="@drawable/ic_arrow_right"
                    android:gravity="center_vertical"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:text="销售日报" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>