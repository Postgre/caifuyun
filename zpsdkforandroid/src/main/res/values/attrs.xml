<resources>

    <attr name="cropImageStyle" format="reference" />
    <attr name="itemValues" format="reference" />
    <attr name="url" format="string" />
    <attr name="loadSize" format="integer" />
    <attr name="listKeyInJson" format="string" />
    <attr name="itemLayout" format="reference" />
    <declare-styleable name="CropImageView">
        <attr name="highlightColor" format="reference|color" />
        <attr name="showThirds" format="boolean" />
        <attr name="showCircle" format="boolean" />
        <attr name="showHandles">
            <enum name="changing" value="0" />
            <enum name="always" value="1" />
            <enum name="never" value="2" />
        </attr>
    </declare-styleable>
    <!-- 计时器CountDownButton属性 -->
    <declare-styleable name="CountDownButton">
        <attr name="endText" format="string" />
        <attr name="unit" format="string" />
        <attr name="millisInFuture" format="integer" />
        <attr name="countDownInterval" format="integer" />
    </declare-styleable>

    <declare-styleable name="CheckableEditText">
        <attr name="name" format="string" />
        <attr name="minSize" format="integer" />
        <attr name="maxSize" format="integer" />
        <attr name="enableCharacter">
            <enum name="number" value="1"></enum>
            <enum name="number_en_underline" value="2"></enum>
        </attr>
    </declare-styleable>

    <!-- AchievePhotoImageView -->
    <declare-styleable name="AchievePhotoImageView">
        <attr name="photoDialogLayout" format="reference" />
        <attr name="deleteDialogLayout" format="reference" />
        <attr name="cropShape">
            <enum name="none" value="1"></enum>
            <enum name="square" value="2"></enum>
            <enum name="rectangle" value="3"></enum>
        </attr>
    </declare-styleable>
    <!-- WheelTextView -->
    <declare-styleable name="WheelTextView">
        <attr name="wheelValues" format="reference" />
        <attr name="wheelHeight" format="dimension" />
    </declare-styleable>
    <!-- SelectPopupView -->
    <declare-styleable name="SelectMenuAttachView">
        <attr name="itemValues"/>
    </declare-styleable>
    <!-- IndicatorListView -->
    <declare-styleable name="IndicatorListView">
        <attr name="itemValues" />
    </declare-styleable>
        <!--ECListViewLayout-->
    <declare-styleable name="ECListViewLayout">
        <attr name="url"/>
        <attr name="loadSize"/>
        <attr name="listKeyInJson"/>
        <attr name="itemLayout"/>
        <attr name="requestMethod">
            <enum name="get" value="0"></enum>
            <enum name="post" value="1"></enum>
        </attr>
        <attr name="refreshComponent">
            <enum name="pullToRefresh" value="0"></enum>
            <enum name="swipe" value="1"></enum>
        </attr>
        <attr name="refreshMode">
            <enum name="up" value="0"></enum>
            <enum name="bottom" value="1"></enum>
            <enum name="both" value="2"></enum>
        </attr>
    </declare-styleable>
</resources>